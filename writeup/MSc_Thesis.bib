@article{reddingIncorporatingEvolutionaryMeasures2006,
  title = {Incorporating {{Evolutionary Measures}} into {{Conservation Prioritization}}},
  author = {Redding, David W. and Mooers, Arne Ø.},
  date = {2006},
  journaltitle = {Conservation Biology},
  volume = {20},
