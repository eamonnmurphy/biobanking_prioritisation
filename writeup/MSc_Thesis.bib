@article{sandlerEthicalAnalysisCloning2021,
  title = {An Ethical Analysis of Cloning for Genetic Rescue: {{Case}} Study of the Black-Footed Ferret},
  shorttitle = {An Ethical Analysis of Cloning for Genetic Rescue},
  author = {Sandler, Ronald L. and Moses, Lisa and Wisely, Samantha M.},
