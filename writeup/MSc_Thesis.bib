@article{davicLinkingKeystoneSpecies2003,
  title = {Linking {{Keystone Species}} and {{Functional Groups}}: {{A New Operational Definition}} of the {{Keystone Species Concept}}},
  shorttitle = {Linking {{Keystone Species}} and {{Functional Groups}}},
  author = {Davic, Robert D.},
